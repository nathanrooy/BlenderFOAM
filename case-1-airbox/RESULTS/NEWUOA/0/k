FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{
    #include "include/fixedInlet"

    airBox_abOutlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    airBox_abWall
    {
        type            kqRWallFunction;
        value           $internalField;
    }

}


